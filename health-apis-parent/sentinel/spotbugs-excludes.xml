<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!--
    IdMeOaut.code() and token() are Lombok generated lazy getter that is implemented
    correctly. Details can be read here: https://projectlombok.org/features/GetterLazy
  -->
  <Match>
    <Class name="gov.va.health.api.sentinel.IdMeOauthRobot"/>
    <Or>
      <Method name="code"/>
      <Method name="token"/>
    </Or>
    <Or>
      <Bug pattern="JLM_JSR166_UTILCONCURRENT_MONITORENTER"/>
      <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
    </Or>
  </Match>
</FindBugsFilter>
